#include<stdio.h>
int main()
{
   float num1,num2,sum,sub,mul,div;	


    printf("Enter first number:");
    scanf("%f", &num1);

    printf("Enter second number:");
    scanf("%f", &num2);


     sum = num1 + num2;
     printf("Sum of %f + %f = %f\n", num1, num2 ,sum);
     
     sub = num1 - num2;
     printf("Sub of %f - %f = %f\n", num1, num2 ,sub);


     mul = num1 * num2;
    printf("mul of %f * %f = %f\n", num1, num2 ,mul);
     

     div = num1 / num2;
     printf("Div of %f / %f = %f\n", num1, num2 ,div);
     
     
     
      return 0;
 }
